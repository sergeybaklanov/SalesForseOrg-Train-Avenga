    <validationRules>
        <fullName>Project_Budget_should_be_populated</fullName>
        <active>true</active>
        <description>Project Budget should be populated if Is Billable = true</description>
        <errorConditionFormula>(Is_Billable__c==true)&amp;&amp;( ISBLANK(Project_Budget__c)||(Project_Budget__c&lt;=0))</errorConditionFormula>
        <errorDisplayField>Project_Budget__c</errorDisplayField>
        <errorMessage>Project Budget should be populated if Is Billable = true</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_Future</fullName>
        <active>true</active>
        <description>should be in the future for new Project (only on create)</description>
        <errorConditionFormula>Start_Date__c &lt; TODAY()&amp;&amp;(ISBLANK(Id))</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>Start Date should be in the future for new Project (only on create)</errorMessage>
    </validationRules>
